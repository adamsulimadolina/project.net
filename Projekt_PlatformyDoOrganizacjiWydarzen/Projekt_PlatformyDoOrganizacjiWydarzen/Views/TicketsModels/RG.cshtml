@model IEnumerable<Projekt_PlatformyDoOrganizacjiWydarzen.Models.VerifyModel>

@{
    ViewData["Title"] = "RG";
}

<h1>RQ</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            
            <th>
                @Html.DisplayNameFor(model => model.event_id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ticket_id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Regis)
            </th>
            
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                
                <td>
                    @Html.DisplayFor(modelItem => item.event_id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ticket_id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Regis)
                </td>
                
            </tr>
        }
    </tbody>
</table>
